{
  "name": "backend-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --ext ts --exec ts-node src/rehouzd/estimator/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/rehouzd/estimator/index.js",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "db:init": "cross-env INITIALIZE_DB=true ts-node src/rehouzd/estimator/config/dbInit.ts",
    "report:buyers": "cross-env NODE_ENV=development ts-node -r dotenv/config src/rehouzd/estimator/scripts/generateBuyerPurchaseReport.ts",
    "report:buyers:fast": "cross-env NODE_ENV=development REPORT_CONCURRENCY=12 ts-node -r dotenv/config src/rehouzd/estimator/scripts/generateBuyerPurchaseReport.ts",
    "report:caprate:resalerental": "cross-env NODE_ENV=development ts-node -r dotenv/config src/rehouzd/estimator/scripts/generateBuyerCapRateResaleRental.ts",
    "report:caprate:resalerental:fast": "cross-env NODE_ENV=development REPORT_CONCURRENCY=12 ts-node -r dotenv/config src/rehouzd/estimator/scripts/generateBuyerCapRateResaleRental.ts",
    "data:tn-property-events": "cross-env NODE_ENV=development ts-node -r dotenv/config src/rehouzd/estimator/scripts/fetchTNPropertyEvents.ts",
    "data:tn-property-events:fast": "cross-env NODE_ENV=development TN_EVENTS_CONCURRENCY=15 ts-node -r dotenv/config src/rehouzd/estimator/scripts/fetchTNPropertyEvents.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@azure/communication-email": "^1.0.0",
    "@azure/identity": "^3.3.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/storage-blob": "^12.13.0",
    "@stripe/stripe-js": "^7.5.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/stripe": "^8.0.416",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^150.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.3",
    "stripe": "^18.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.13.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
